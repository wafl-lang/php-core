{
  "name": "wafl/php-core",
  "description": "PHP implementation of the WAFL core",
  "homepage": "https://github.com/wafl-lang/php-core",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "wafl",
    "configuration",
    "yaml",
    "dsl"
  ],
  "authors": [
    {
      "name": "Jornatf",
      "homepage": "https://github.com/jornatf"
    }
  ],
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Wafl\\Core\\": "src/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  },
  "autoload-dev": {
    "psr-4": {
      "Wafl\\Core\\Tests\\": "tests/"
    }
  },
  "support": {
    "issues": "https://github.com/wafl-lang/php-core/issues"
  }
}
